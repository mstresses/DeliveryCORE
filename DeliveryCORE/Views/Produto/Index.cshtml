
@{
    ViewData["Title"] = "Index";
}

<h1>Produtos Cadastrados</h1>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>Valor</th>
    </tr>
    @foreach (var item in ViewBag.Produtos)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Nome</td>
            <td>@item.Valor</td>
        </tr>
    }
</table>

<!DOCTYPE html>